    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/invoke-ai-ui.umd.js",
  "module": "./dist/invoke-ai-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/invoke-ai-ui.es.js",
      "require": "./dist/invoke-ai-ui.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
