    "typegen": "node scripts/typegen.js",
    "preview": "vite preview",
    "lint:knip": "knip --tags=-knipignore",
    "lint:dpdm": "dpdm --no-warning --no-tree --transform --exit-code circular:1 src/main.tsx",
